# ... inside rule_type == 'API_RESPONSE' ...
                result = self._evaluate_condition(rule, self.current_context, prev_rule_id, context_rule, rule)

                # ğŸ”´ START DEBUGGING & FIX BLOCK ğŸ”´
                if rule.get('rule_name') == 'portin-check':
                    print("\nğŸ›‘ --- DEBUGGING PORT-IN DATA STRUCTURE ---")
                    
                    # 1. Check if Context exists
                    if 'api_result_RULE_200' in self.current_context:
                        print("   âœ… Found 'api_result_RULE_200' in context")
                        api_data = self.current_context['api_result_RULE_200'].get('response_data', {})
                        
                        # 2. Check orderDetails
                        if 'orderDetails' in api_data:
                            od = api_data['orderDetails']
                            print(f"   âœ… Found 'orderDetails'. Type: {type(od)}")
                            print(f"   ğŸ“‚ RAW KEYS inside orderDetails: {list(od.keys())}")
                            
                            # 3. Check specific value
                            val = od.get('portIn', 'NOT_FOUND')
                            print(f"   ğŸ” Value of 'portIn': {val}")
                            
                            # 4. Perform Manual Override Logic
                            actual_val = str(val).lower().strip()
                            if actual_val in ['not_found', '0', 'false', 'none', '', 'null']:
                                print("   ğŸ‘‰ Decision: Normal/Active Order (Continue)")
                                result = True # FORCE CONTINUE
                            elif actual_val != 'a':
                                print("   ğŸ‘‰ Decision: Pending/Error Order (Stop)")
                                # Execute STOP Logic
                                template_data = self._get_response_template(rule['rule_context'])
                                return {
                                    "status": "SUCCESS",
                                    "message": template_data['message_content'],
                                    "response": template_data['message_content'],
                                    "rule_id": rule['rule_id']
                                }
                            else:
                                print("   ğŸ‘‰ Decision: Status is 'A' (Continue)")
                                result = True # FORCE CONTINUE
                        else:
                            print("   âŒ 'orderDetails' key NOT found in API data")
                    else:
                        print("   âŒ 'api_result_RULE_200' NOT found in context keys:", list(self.current_context.keys()))
                    
                    print("ğŸ›‘ --- END DEBUGGING ---\n")
                # ğŸ”´ END DEBUGGING & FIX BLOCK ğŸ”´

                # ... existing code (if result:) ...
